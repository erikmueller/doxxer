{
    "rules": {
        "no-cond-assign": 2,
        "no-comma-dangle": 2,
        "no-underscore-dangle": 0,
        "no-dupe-keys": 2,
        "no-extra-semi": 1,
        "no-debugger": 1,
        "no-unreachable": 2,
        "valid-typeof": 2,

        "default-case": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-caller": 2,
        "no-else-return": 2,
        "no-eval": 2,
        "no-multi-str": 2,
        "no-new": 2,
        "no-new-wrappers": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-self-compare": 2,
        "no-with": 2,
        "wrap-iife": 2,

        "strict": 2,
        "no-extra-strict": 2,

        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,

        "no-mixed-requires": 0,
        "no-path-concat": 2,
        "no-sync": 2,

        "brace-style": [1, "1tbs"],
        "camelcase": 1,
        "consistent-this": [2, "self"],
        "eol-last": 2,
        "no-lonely-if": 2,
        "new-cap": [2, true, true, ["this"]],
        "new-parens": 2,
        "no-space-before-semi": 1,
        "no-trailing-spaces": 1,
        "no-mixed-spaces-and-tabs": 2,
        "quotes": [2, "single", "avoid-escape"],
        "semi": [1, "always"],
        "space-after-keywords": [1, "always"],
        "space-in-brackets": [1, "never"],
        "space-infix-ops": 1,
        "space-return-throw-case": 1,
        "vars-on-top": 2,

        "no-bitwise": 2,
        "no-plusplus": 2
    }
}
